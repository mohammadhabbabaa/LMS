@model IEnumerable<Kurs.Models.LiveCours>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ZoomUser)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ZoomPass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cours.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ZoomUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZoomPass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cours.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
